{
  "DefaultConnection": "Data Source=joskashop.db",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Debug"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/JoskaShop.log",
          "restrictedToMinimumLevel": "Debug"
        }
      }
    ]
  }
}